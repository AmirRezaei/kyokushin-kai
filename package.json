  "private": true,
  "scripts": {
    "build": "tsc -b && vite build",
    "cf-typegen": "wrangler types",
    "check": "tsc && vite build && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "secrets:put": "bun scripts/put-all-secrets.ts",
    "db:create": "wrangler d1 migrations create DB",
    "db:dev:migrate": "wrangler d1 migrations apply DB --local",
    "db:prod:migrate": "wrangler d1 migrations apply DB --remote",
    "db:dev:seed:techni ques": "bun scripts/seed_techniques.ts && wrangler d1 execute DB --local --file scripts/seed_techniques.sql",
    "db:prod:seed:techniques": "bun scripts/seed_techniques.ts && wrangler d1 execute DB --remote --file scripts/seed_techniques.sql",
    "db:dev:seed:quotes": "bun scripts/seed_quotes.ts && wrangler d1 execute DB --local --file scripts/seed_quotes.sql",
    "db:prod:seed:quotes": "bun scripts/seed_quotes.ts && wrangler d1 execute DB --remote --file scripts/seed_quotes.sql",
    "dev": "vite dev --port 3000",
    "lint": "eslint .",
    "preview": "bun run build && vite preview",
    "test": "bun test",
    "dev:api": "wrangler dev",
    "dev:all": "concurrently \"bun run dev\" \"bun run dev:api\""
  },
  "type": "module"
}
